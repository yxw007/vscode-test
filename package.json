{
  "name": "vscode-test",
  "displayName": "vscode-test",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.95.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "myExtensionSidebar",
          "title": "My Extension",
          "icon": "icon.png"
        }
      ]
    },
    "views": {
      "myExtensionSidebar": [
        {
          "id": "Translate.Logs",
          "name": "Logs"
        }
      ]
    },
    "commands": [
      {
        "command": "myExtension.sortLogsAsc",
        "title": "Sort Logs Asc",
        "icon": "$(sort-precedence)"
      },
      {
        "command": "myExtension.sortLogsDesc",
        "title": "Sort Logs Desc",
        "icon": "$(sort-precedence)"
      },
      {
        "command": "myExtension.clearLogs",
        "title": "Clear Logs",
        "icon": "$(trash)"
      },
      {
        "command": "myExtension.addRandomLog",
        "title": "Add Random Log",
        "icon": "$(add)"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "myExtension.sortLogsAsc",
          "when": "view == logView",
          "group": "navigation"
        },
        {
          "command": "myExtension.sortLogsDesc",
          "when": "view == logView",
          "group": "navigation"
        },
        {
          "command": "myExtension.clearLogs",
          "when": "view == logView",
          "group": "navigation"
        },
        {
          "command": "myExtension.addRandomLog",
          "when": "view == logView",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.95.0",
    "@types/mocha": "^10.0.9",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.7.0",
    "eslint": "^9.13.0",
    "esbuild": "^0.24.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.6.3",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
